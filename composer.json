{
    "name": "socieboy/filemanager",
    "description": "Easy and Free File Manager for your Projects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Sepulveda",
            "email": "socieboy@gmail.com"
        }
    ],
    "homepage": "https://github.com/socieboy/filemanager",
    "keywords": ["Socieboy", "Laravel", "FileManager"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "laravel/helpers": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Socieboy\\FileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Socieboy\\FileManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Socieboy\\FileManager\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Socieboy\\FileManager\\Facades\\FileManager"
            }
        }
    }
}
